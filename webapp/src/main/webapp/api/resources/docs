        </span><span class="p">{</span><span class="w">
            </span><span class="s2">"id"</span><span class="p">:</span><span class="w"> </span><span class="mi">4</span><span class="p">,</span><span class="w">
            </span><span class="s2">"name"</span><span class="p">:</span><span class="w"> </span><span class="s2">"PEOPLE"</span><span class="p">,</span><span class="w">
            </span><span class="s2">"production"</span><span class="p">:</span><span class="w"> </span><span class="mf">0.315</span><span class="w">
