            </span><span class="s2">"users_url"</span><span class="p">:</span><span class="w"> </span><span class="s2">"http://localhost:8080/api/v1/clans/13/users"</span><span class="p">,</span><span class="w">
            </span><span class="s2">"wins"</span><span class="p">:</span><span class="w"> </span><span class="mi">2</span><span class="p">,</span><span class="w">
